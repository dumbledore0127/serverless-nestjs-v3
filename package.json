{
  "name": "serverless-nestjs-v3",
  "packageManager": "yarn@3.4.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "husky install",
    "format": "prettier --ignore-path .gitignore --ignore-unknown --check \"**/*\"",
    "backend": "yarn workspace backend",
    "openapi": "yarn workspace @serverless-nestjs-v3/openapi",
    "web": "yarn workspace web"
  },
  "devDependencies": {
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "prettier": "2.8.4"
  }
}
